package Definicoes;

import java.lang.annotation.Target;


import Pagina.InMetrics;

public class DefAcesso {
	
	InMetrics inmetrics = new InMetrics();
	
	@Given("Estar na pagina  InMetrics")
	public void AbrirPagina(){
		
		inmetrics.AbreNavegador();	
		
	}
	
	@When("Clicar na pagina de Carreiras")
	public void PressionaOpcaoCarreiras(){
		
		inmetrics.AcessoAutomatico1();
		
	}
	
	@Then("Acionar o bot√£o 'confira nossas vagas' ")
	public void PressionaBotao(){
		
		inmetrics.AcessoAutomatico2();
		
	}
}
